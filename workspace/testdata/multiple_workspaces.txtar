# setup repo
mkdir repo
cd repo
exec jj git init

# acquire two workspaces
exec $INCR workspace acquire --purpose feature-one
envset WORKSPACE_ONE stdout

exec $INCR workspace acquire --purpose feature-two
! stdout ${WORKSPACE_ONE@R}
envset WORKSPACE_TWO stdout

exists $WORKSPACE_ONE
exists $WORKSPACE_TWO

exec $INCR workspace list
stdout 'ws-001'
stdout 'ws-002'
stdout 'acquired'
stdout 'feature-one'
stdout 'feature-two'
