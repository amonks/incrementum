# setup repo
mkdir repo
cd repo
exec jj git init

exec $II workspace acquire --purpose destroy-one
envset WORKSPACE_ONE stdout

exec $II workspace acquire --purpose destroy-two
envset WORKSPACE_TWO stdout

exists $WORKSPACE_ONE
exists $WORKSPACE_TWO

exec $II workspace list
stdout 'ws-001'
stdout 'ws-002'

exec $II workspace destroy-all

! exists $WORKSPACE_ONE
! exists $WORKSPACE_TWO

exec $II workspace list
stdout 'No workspaces found'
